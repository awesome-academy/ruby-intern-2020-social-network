<div>
  <div class="rounded bg-white shadow mb-3 px-5 py-4 profile-about">
    <h1 class="gapo-title mb-3 pt-2"><%= t "introduction" %></h1>
    <div class="about-form__title font-weight-semi-bold py-3 d-flex align-items-center">
      <%= t "information" %>
    </div>
    <%= form_for [:users, IntroUser.new] do |f| %>
      <%= render "form_info", f: f %>
      <%= f.submit t("save") %>
    <% end %>
  </div>
</div>
