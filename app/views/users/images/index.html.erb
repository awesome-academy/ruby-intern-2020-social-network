<div>
  <div class="d-flex align-items-center mb-3 pt-2">
    <%= link_to t("add_image"), new_users_user_image_path, class: "btn btn-sm mr-2 d-flex align-items-center ml-auto btn-white border" %>
  </div>
  <div class="rounded bg-white shadow mb-3 p-3">
    <h1 class="gapo-title"><%= t "image" %></h1>
    <div>
      <div class="row">
        <div class="col-3">
          <div class="profile-photo-item position-relative">
            <% @images.each do |image| %>
              <%= image_tag image, size: "250x250", class: "gapo-thumbnail gapo-thumbnail--1x1" %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
