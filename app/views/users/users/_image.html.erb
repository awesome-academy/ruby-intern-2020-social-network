<div class="rounded bg-white shadow mb-3 p-3">
  <div class="d-flex align-items-center">
    <div class="gapo-title timeline__title"><%= t "image" %></div>
    <%= link_to t("see_all"), "#", class: "ml-auto text-primary" %>
  </div>
  <div class="row mt-2">
    <% if user.images.attached? %>
      <% user.images.each do |image| %>
        <div class="col-4">
          <div class="gapo-thumbnail gapo-thumbnail--1x1 mb-2"><%= image_tag image, size: Settings.size_user_images %></div>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
