<div class="form-sign_in_up">
  <div class="greeting"><%= t ".forgot_passsword" %></div>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= form_for :password_reset, url: users_password_resets_path do |f| %>
        <%= f.label t(".email") %>
        <%= f.email_field :email, class: "form-control" %>
        
        <%= f.submit t(".submit"), class: "submit-sign_in_up" %>
      <% end %>
    </div>
  </div>
</div>
